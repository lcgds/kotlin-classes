<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.senac.aula03">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Aula03">
        <activity
            android:name=".QuestionActivity"
            android:exported="true"
            android:parentActivityName=".QuizActivity">
            <intent-filter>
                <action android:name="br.senac.aula03.QuestionActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".QuizActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="br.senac.aula03.QuizActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="Atividades">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ImageActivity"
            android:exported="true"
            android:label="Imagem">
            <intent-filter>
                <action android:name="br.senac.aula03.ImageActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataHoraActivity"
            android:exported="true"
            android:label="Data e hora" />
        <activity
            android:name=".TipoTelefoneActivity"
            android:exported="true"
            android:label="Tipo de telefone" />
        <activity
            android:name=".PreferenciaContatoActivity"
            android:exported="true"
            android:label="PreferÃªncia de contato" />
        <activity
            android:name=".ExemploPronome"
            android:exported="true"
            android:label="Pronome"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>